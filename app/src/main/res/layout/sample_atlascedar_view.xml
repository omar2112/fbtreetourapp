<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res/com.example.bluefox.listtest1"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.example.bluefox.listtest1.atlascedarView
        android:layout_width="246dp"
        android:layout_height="210dp"
        android:background="#ccc"
        android:paddingBottom="40dp"
        android:paddingLeft="20dp"
        app:exampleColor="#33b5e5"
        app:exampleDimension="24sp"
        app:exampleDrawable="@android:drawable/ic_menu_add"
        app:exampleString="Hello, atlascedarView"
        android:id="@+id/view" />

    <ImageView
        android:layout_width="224dp"
        android:layout_height="175dp"
        android:id="@+id/imageView9"
        android:layout_gravity="left|top"
        android:src="@drawable/atlascedar"
        android:layout_below="@+id/textView15"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Atlas Cedar"
        android:id="@+id/textView15"
        android:layout_gravity="left|top"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/imageView9"
        android:layout_alignEnd="@+id/imageView9" />

</RelativeLayout>
